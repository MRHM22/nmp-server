					<handler function="OnButton_htmlBrowserClick" entry="EVT_BUTTON" />
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxButton" name="ID_BUTTON7" variable="Button_htmlOpen" member="yes">
					<label>Open...</label>
					<handler function="OnButton_htmlOpenClick" entry="EVT_BUTTON" />
				</object>
				<flag>wxALL|wxEXPAND|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
		</object>
		<object class="wxTimer" name="ID_TIMER1" variable="Timer_checkProcess" member="yes">
			<interval>1000</interval>
			<handler function="OnTimer_checkProcessTrigger" entry="EVT_TIMER" />
		</object>
	</object>
</wxsmith>
